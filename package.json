{
  "name": "understanding-transpilers-and-bundlers",
  "version": "0.0.1",
  "description": "Playground to understand the code generated from different transpilers.",
  "author": "nosolosw",
  "license": "GPL-3.0",
  "main": "index.js",
  "scripts": {
    "do": "run-p do:*",
    "do:arrow-functions": "run-p do:arrow-functions:*",
    "do:arrow-functions:babel": "babel src/arrow-functions/input.js -o src/arrow-functions/output.babel.js --presets=env",
    "do:arrow-functions:closure": "google-closure-compiler src/arrow-functions/input.js --formatting PRINT_INPUT_DELIMITER --formatting SINGLE_QUOTES --formatting PRETTY_PRINT > src/arrow-functions/output.closure.js",
    "do:cjs-to-browser": "run-p do:cjs-to-browser:*",
    "do:cjs-to-browser:rollup": "rollup -c src/cjs-to-browser/rollup.config.js",
    "do:cjs-to-browser:webpack": "webpack --config src/cjs-to-browser/webpack.babel.js",
    "do:to-iffe": "run-p do:to-iffe:*",
    "do:to-iffe:esm": "run-p do:to-iffe:esm:*",
    "do:to-iffe:esm:rollup": "rollup -c src/to-iffe/esm.config.rollup.js",
    "do:to-iffe:esm:webpack": "webpack --config src/to-iffe/esm.config.webpack.babel.js",
    "do:exports": "run-p do:exports:*",
    "do:exports:default": "run-p do:exports:default:*",
    "do:exports:default:babel": "babel src/exports/default.input.js -o src/exports/default.output.babel.js --presets=env",
    "do:exports:default:closure": "google-closure-compiler src/exports/default.input.js --formatting PRINT_INPUT_DELIMITER --formatting SINGLE_QUOTES --formatting PRETTY_PRINT > src/exports/default.output.closure.js",
    "do:exports:named": "run-p do:exports:named:*",
    "do:exports:named:babel": "babel src/exports/named.input.js -o src/exports/named.output.babel.js --presets=env",
    "do:exports:named:closure": "google-closure-compiler src/exports/named.input.js --module_resolution=NODE --formatting PRINT_INPUT_DELIMITER --formatting SINGLE_QUOTES --formatting PRETTY_PRINT > src/exports/named.output.closure.js",
    "do:exports:both": "run-p do:exports:both:*",
    "do:exports:both:babel": "babel src/exports/both.input.js -o src/exports/both.output.babel.js --presets=env",
    "do:exports:both:closure": "google-closure-compiler src/exports/both.input.js --module_resolution=NODE --formatting PRINT_INPUT_DELIMITER --formatting SINGLE_QUOTES --formatting PRETTY_PRINT > src/exports/both.output.closure.js",
    "do:imports": "run-p do:imports:*",
    "do:imports:default": "run-p do:imports:default:*",
    "do:imports:default:babel": "babel src/imports/default.input.js -o src/imports/default.output.babel.js --presets=env",
    "do:imports:default:closure": "google-closure-compiler src/imports/default.input.js src/exports/default.input.js --module_resolution=NODE --formatting PRINT_INPUT_DELIMITER --formatting SINGLE_QUOTES --formatting PRETTY_PRINT > src/imports/default.output.closure.js",
    "do:imports:named": "run-p do:imports:named:*",
    "do:imports:named:babel": "babel src/imports/named.input.js -o src/imports/named.output.babel.js --presets=env",
    "do:imports:named:closure": "google-closure-compiler src/imports/named.input.js src/exports/named.input.js --module_resolution=NODE --formatting PRINT_INPUT_DELIMITER --formatting SINGLE_QUOTES --formatting PRETTY_PRINT > src/imports/named.output.closure.js",
    "do:imports:both": "run-p do:imports:both:*",
    "do:imports:both:babel": "babel src/imports/both.input.js -o src/imports/both.output.babel.js --presets=env",
    "do:imports:both:closure": "google-closure-compiler src/imports/both.input.js src/exports/named.input.js src/exports/default.input.js --module_resolution=NODE --formatting PRINT_INPUT_DELIMITER --formatting SINGLE_QUOTES --formatting PRETTY_PRINT > src/imports/both.output.closure.js",
    "do:imports:both:rollup": "rollup src/imports/both.input.js -o src/imports/both.output.rollup.js -f cjs",
    "do:imports:both:webpack": "webpack --config src/imports/webpack.babel.js",
    "predo:react-analyze-bundles": "webpack --config src/react-helloworld/webpack.babel.prod.scope.uglify.js --profile --json > stats.json",
    "do:react-analyze-bundles": "webpack-bundle-analyzer stats.json .",
    "do:react": "run-p do:react:*",
    "do:react:babel": "babel --plugins syntax-jsx,transform-react-jsx src/react-helloworld/input.jsx -o src/react-helloworld/output.babel.js",
    "do:react:webpack": "webpack --config src/react-helloworld/webpack.babel.js",
    "do:react:webpack-production": "webpack --config src/react-helloworld/webpack.babel.prod.js",
    "do:react:webpack-production-scope": "webpack --config src/react-helloworld/webpack.babel.prod.scope.js",
    "do:react:webpack-production-scope-uglify": "webpack --config src/react-helloworld/webpack.babel.prod.scope.uglify.js",
    "do:tree-shaking": "run-p do:tree-shaking:*",
    "do:tree-shaking:babel": "babel src/tree-shaking/input.js -o src/tree-shaking/output.babel.js --presets=env",
    "do:tree-shaking:babel-minify": "minify src/tree-shaking/input.js -o src/tree-shaking/output.babel-minify.js",
    "do:tree-shaking:rollup": "rollup src/tree-shaking/input.js -o src/tree-shaking/output.rollup.js -f cjs",
    "do:tree-shaking:webpack": "webpack --config src/tree-shaking/webpack.babel.js",
    "do:tree-shaking:webpack-minify": "webpack --config src/tree-shaking/webpack.babel.minify.js",
    "do:tree-shaking:webpack-minify-scopehoisting": "webpack --config src/tree-shaking/webpack.babel.minify.scopehoisting.js",
    "do:tree-shaking:webpack-scopehoisting": "webpack --config src/tree-shaking/webpack.babel.scopehoisting.js",
    "do:tree-shaking:webpack-uglify": "webpack --config src/tree-shaking/webpack.babel.uglify.js",
    "do:tree-shaking:webpack-uglify-scopehoisting": "webpack --config src/tree-shaking/webpack.babel.uglify.scopehoisting.js",
    "do:css": "run-p do:css:*",
    "do:css:tree-shaking": "node-sass src/css/tree-shaking/input.scss > src/css/tree-shaking/output.scss",
    "do:css:import": "node-sass src/css/import/input.scss > src/css/import/output.scss"
  },
  "dependencies": {
    "babel-cli": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-minify": "0.2.0",
    "babel-minify-webpack-plugin": "0.2.0",
    "babel-plugin-syntax-jsx": "6.18.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-env": "1.6.1",
    "google-closure-compiler": "20171112.0.0",
    "node-sass": "4.7.2",
    "npm-run-all": "4.1.2",
    "npx": "9.7.1",
    "prettier": "1.8.2",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "rollup": "0.52.1",
    "uglifyjs-webpack-plugin": "1.1.4",
    "webpack": "3.10.0",
    "webpack-bundle-analyzer": "2.9.1"
  },
  "devDependencies": {
    "rollup-plugin-commonjs": "^8.3.0"
  }
}
